{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
        <img width="20" height="28" alt="Tbbc Cache"
             src="data:image/png;base64," />
        <span class="sf-toolbar-status">{{ collector.results | length }}</span>
    {% endset %}

    {% set text %}
        <div class="sf-toolbar-info-piece">
            <b>Cache operations</b>
            <span class="sf-toolbar-status" style="background-color: transparent; color: #666">
                {{ collector.results | length }}
            </span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>Cache hits</b>
            <span class="sf-toolbar-status{% if collector.hits > 0 %} sf-toolbar-status-green{% endif %}">
                {{- collector.hits -}}
            </span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>Cache miss</b>
            <span class="sf-toolbar-status{% if collector.miss > 0 %} sf-toolbar-status-yellow{% endif %}">
                {{- collector.miss -}}
            </span>
        </div>
    {% endset %}

    {% include 'WebProfilerBundle:Profiler:toolbar_item.html.twig' with { 'link': profiler_url } %}
{% endblock %}

{% block menu %}
    {# the menu content #}
    <span class="label">
        {% spaceless %}
            <span class="icon">
            {#
                Icons License
                =============

                Glyphish icons (http://glyphish.com/) created by Joseph Wain
                (http://www.penandthink.com/) are shared under a Creative Commons Attribution
                license (http://creativecommons.org/licenses/by/3.0/us/).
            #}
                <img src="data:image/png;base64," style="padding-top:5px;" alt="Tbbc Cache" />
        </span>
        {% endspaceless %}
        <strong>Tbbc Cache</strong>
        <span class="count">
            <span>{{ collector.results | length }}</span>
        </span>
    </span>
{% endblock %}

{% block panel %}
    {{ block('overview') }}
    {{ block('cacheable') }}
    {{ block('cache_evict') }}
    {{ block('cache_update') }}
{% endblock %}

{% block overview %}
    <h2>Overview</h2>

    {% if collector.results %}
        {%
        set data = {
        'Total cache operations': collector.results|length,
        'Cacheable operations': collector.cacheableResults|length,
        'CacheEvict operations': collector.cacheaEvictResults|length,
        'CacheUpdate operations': collector.cacheUpdateResults|length
        }
        %}

        {% include 'WebProfilerBundle:Profiler:table.html.twig' with {'data': data} only %}
    {% else %}
        <p>
            <em>No cache operations</em>
        </p>
    {% endif %}
{% endblock overview %}

{% block cacheable %}
    <h2>Cacheable Operations</h2>

    {% if collector.cacheableOperations %}
        <table>
            <thead>
            <tr>
                <th scope="col">Class</th>
                <th scope="col">Method</th>
                <th scope="col">Key</th>
                <th scope="col">Hit</th>
                <th scope="col">Miss</th>
                <th scope="col">Pools</th>
            </tr>
            </thead>
            <tbody>
            {% for result in collector.cacheableResults %}
                <tr>
                    <td>{{ result.className }}</td>
                    <td>{{ result.method }}</td>
                    <td>{{ result.key }}</td>
                    <td>{{ (result.cacheHit) ? '1' : '0' }}</td>
                    <td>{{ (not result.cacheHit) ? '1' : '0' }}</td>
                    <td>
                        <ul>
                            {% for pool in result.pools %}
                                <li>{{ pool }}</li>
                            {% endfor %}
                        </ul>
                    </td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    {% else %}
        <p>
            <em>No Cacheable operations</em>
        </p>
    {% endif %}
{% endblock cacheable %}

{% block cache_evict %}
    <h2>CacheEvict Operations</h2>

    {% if collector.cacheaEvictResults %}
        <table>
            <thead>
            <tr>
                <th scope="col">Class</th>
                <th scope="col">Method</th>
                <th scope="col">Key</th>
                <th scope="col">Tags</th>
                <th scope="col">Pools</th>
            </tr>
            </thead>
            <tbody>
            {% for result in collector.cacheaEvictResults %}
                <tr>
                    <td>{{ result.className }}</td>
                    <td>{{ result.method }}</td>
                    <td>{{ (result.key is not empty) ? operation.key : ' - '  }}</td>
                    <td>
                        <ul>
                            {% for tag in result.tags %}
                                <li>{{ tag }}</li>
                            {% endfor %}
                        </ul>
                    </td>
                    <td>
                        <ul>
                            {% for pool in operation.pools %}
                                <li>{{ pool }}</li>
                            {% endfor %}
                        </ul>
                    </td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    {% else %}
        <p>
            <em>No CacheEvict operations</em>
        </p>
    {% endif %}
{% endblock cache_evict %}

{% block cache_update %}
    <h2>CacheUpdate Operations</h2>

    {% if collector.cacheUpdateResults %}
        <table>
            <thead>
            <tr>
                <th scope="col">Class</th>
                <th scope="col">Method</th>
                <th scope="col">Key</th>
                <th scope="col">Caches</th>
            </tr>
            </thead>
            <tbody>
            {% for operation in collector.cacheUpdateOperations %}
                <tr>
                    <td>{{ result.className }}</td>
                    <td>{{ result.method }}</td>
                    <td>{{ (result.key }}</td>
                    <td>
                        <ul>
                            {% for pool in operation.pools %}
                                <li>{{ pool }}</li>
                            {% endfor %}
                        </ul>
                    </td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    {% else %}
        <p>
            <em>No CacheUpdate operations</em>
        </p>
    {% endif %}
{% endblock cache_update %}
